@{ 
    Validation.RequireField("username", "Email Address is required");
    Validation.RequireField("password", "Password is required");
    
    Validation.RequireField("first_name", "First Name is required");
    Validation.RequireField("last_name", "Last Name is required");  
    Validation.RequireField("email", "Email Address is required");
    Validation.RequireField("address", "Address is required");
    Validation.RequireField("date_of_birth", "Date of Birth is required");
   
    
    
   
    var username = "";
    var password = "";
    var confirm_pw = "";
    var first_name = "";
    var last_name = "";
    var email = "";
    var address = "";
    var date_of_birth = "";
       
 
    if(IsPost && Validation.IsValid()){
       
        username = Request.Form["username"];
        password = Request.Form["password"];
        first_name = Request.Form["first_name"];
        last_name = Request.Form["last_name"];
        email = Request.Form["email"];
        address = Request.Form["address"];
        date_of_birth = Request.Form["date_of_birth"];
 
        var db = Database.Open("database");
        var insertCommand = "INSERT INTO Users (username, password, first_name, last_name, email, address, date_of_birth) VALUES(@0, @1, @2, @3, @4, @5, @6)";
        db.Execute(insertCommand, username, password, first_name, last_name, email, address, date_of_birth);
        Response.Redirect("home.html");
    }

   
    
}
 
